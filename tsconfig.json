{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "types": ["@cloudflare/workers-types"],
    "forceConsistentCasingInFileNames": true
  },
  "include": ["worker/**/*.ts"]
}